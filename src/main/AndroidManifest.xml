<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ve.edu.ucab.modula.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ve.edu.ucab.modula.app.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ve.edu.ucab.modula.app.FrasesActivity"
            android:label="@string/app_name"
            android:parentActivityName="ve.edu.ucab.modula.app.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ve.edu.ucab.modula.app.MainActivity" />
        </activity>
        <activity
            android:name="ve.edu.ucab.modula.app.Chat"
            android:label="@string/title_activity_chat" >
        </activity>
    </application>

</manifest>
